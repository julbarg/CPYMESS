<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
        http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">

	<navigation-rule>
		<display-name>login.xhtml</display-name>
		<from-view-id>/pages/login.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{login.authenticate}</from-action>
			<from-outcome>true</from-outcome>
			<to-view-id>/pages/ivr.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>cpymes.xhtml</display-name>
		<from-view-id>/pages/cpymes.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{cpymes.goIvr}</from-action>
			<from-outcome>logIn</from-outcome>
			<to-view-id>/pages/login.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>cpymes.xhtml</display-name>
		<from-view-id>/pages/cpymes.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{cpymes.goIvr}</from-action>
			<from-outcome>ivr</from-outcome>
			<to-view-id>/pages/ivr.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>ivr.xhtml</display-name>
		<from-view-id>/pages/ivr.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{ivr.goCpymes}</from-action>
			<from-outcome>true</from-outcome>
			<to-view-id>/pages/cpymes.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>ivr.xhtml</display-name>
		<from-view-id>/pages/ivr.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{ivr.gogoLogIn}</from-action>
			<from-outcome>true</from-outcome>
			<to-view-id>/pages/login.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

</faces-config>
